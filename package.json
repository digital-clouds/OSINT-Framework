{
  "name": "osint-framework",
  "version": "1.0.0",
  "description": "Open Source Intelligence - Reconnaissance Framework",
  "main": "public/index.html",
  "scripts": {
    "clean": "pnpm exec rm -rf node_modules build pnpm-lock.yaml",
    "preinstall": "test \"$CF_PAGES\" = 1 && npx pnpm i || echo Install not required..",
    "upgrade": "pnpm upgrade -i -L",
    "postinstall": "copyfiles -f ./node_modules/d3/d3.min.js ./public/js/vendor/d3",
    "fmt": "trunk fmt",
    "dev": "wrangler dev --assets public/ --latest",
    "check": "trunk check"
  },
  "dependencies": {
    "copyfiles": "2.4.1",
    "d3": "7.8.2"
  },
  "devDependencies": {
    "@trunkio/launcher": "1.2.3",
    "wrangler": "^2.8.1"
  },
  "packageManager": "pnpm@7.26.3",
  "pnpm": {
    "overrides": {
      "http-cache-semantics@<4.1.1": ">=4.1.1"
    }
  }
}
